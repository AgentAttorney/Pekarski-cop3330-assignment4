      <Button mnemonicParsing="false" onAction="#ButtonOKClicked" text="Confirm" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#ButtonOKClicked" text="Return" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</GridPane>
